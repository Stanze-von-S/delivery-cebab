<div class="categoriesList">
  <h2 class="text-center">Категории</h2>
  <div class="list">
    {{#each categories}}
    {{!-- <li class="listItem"> --}}
      <input data-category_index="{{this.id}}" name="my_btn" class="appendBtn btn btn-outline-success" type="button"
        value="{{this.title}}">
    {{!-- </li> --}}
    {{/each}}
  </div>

  <div id="category"></div>
  <br>

  <ul class="list text-center">

    {{!-- <div>
      <button type="button" id="getCoordinates" >Где поесть</button>
    </div> --}}
    
    <a href="https://99px.ru/sstorage/53/2015/06/tmb_134918_7760.jpg"><input class=" btn btn-outline-info" type="button"
        value="Здесь самый вкусный Кебабабчик"></a>

    <div class="radiusSearchGroup">
      <p>Радиус поиска:
        <input type="range" min="100" max="10000" step="100" name="radiusSearch" id="radiusSearch" value="0">
      </p>
    </div>

    <div id="map"></div>
  </ul>

</div>

<script defer src="https://api-maps.yandex.ru/2.1/?apikey=c1dc2a25-2ea2-4b3e-87a8-d091b5e6ebb7&lang=ru_RU" type="text/javascript"></script>
<script defer src="/js/map.js"></script>
